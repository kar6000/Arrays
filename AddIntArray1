package com.oct30;

import java.util.Arrays;

public class AddIntArray {

	public static void main(String[] args) {
		int a[] = {1,2,3,4,5};
		int b[] = {2,3,4,5,6};
		sum(a,b);
	}
	
	public static void sum(int a[], int b[]) {
		for(int i=0; i<a.length; i++) {
			a[i]= a[i]+b[i];
		}
		System.out.println("sum is "+ Arrays.toString(a));
	}

}
